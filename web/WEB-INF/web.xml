<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>ViewSymptoms</servlet-name>
        <servlet-class>Controller.ViewSymptoms</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DiseaseIdentifier</servlet-name>
        <servlet-class>Controller.DiseaseIdentifier</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ViewSymptoms</servlet-name>
        <url-pattern>/ViewSymptoms</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DiseaseIdentifier</servlet-name>
        <url-pattern>/DiseaseIdentifier</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>/View/index.html</welcome-file>
    </welcome-file-list>
</web-app>
